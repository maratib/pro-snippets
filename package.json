{
  "name": "xpro-snippets",
  "displayName": "xpro-snippets",
  "description": "Typescript, Vue, Nuxt, React, Next, Angular, Nest, Express snippets (all in one power pack)",
  "version": "0.0.2",
  "icon": "images/xpro-snippets.png",
  "main": "./out/src/extension",
  "browser": "./out/src/extension",
  "activationEvents": [
    "onCommand:extension.stayClassy"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/maratib/xpro-snippets.git"
  },
  "author": {
    "name": "Maratib Ali Khan",
    "email": "maratib@gmail.com",
    "url": "https://maratib.github.io"
  },
  "publisher": "maratib",
  "license": "ISC",
  "engines": {
    "vscode": "^1.63.0"
  },
  "categories": [
    "Snippets"
  ],
  "contributes": {
    "snippets": [
      {
        "language": "javascript",
        "path": "./snippets/snippets.code-snippets"
      },
      {
        "language": "typescript",
        "path": "./snippets/typescript.code-snippets"
      },
      {
        "language": "typescript",
        "path": "./snippets/typescript-vue.code-snippets"
      },
      {
        "language": "typescript",
        "path": "./snippets/typescript-vue3.code-snippets"
      },
      {
        "language": "typescript",
        "path": "./snippets/nuxt.code-snippets"
      },
      {
        "language": "javascript",
        "path": "./snippets/nuxt.code-snippets"
      },
      {
        "language": "typescriptreact",
        "path": "./snippets/typescriptreact.code-snippets"
      },
      {
        "language": "vue",
        "path": "./snippets/vue.code-snippets"
      },
      {
        "language": "vue-html",
        "path": "./snippets/vue.code-snippets"
      },
      {
        "language": "vue",
        "path": "./snippets/nuxt.code-snippets"
      },
      {
        "language": "vue-html",
        "path": "./snippets/nuxt.code-snippets"
      }
    ],
    "commands": [
      {
        "command": "extension.stayClassy",
        "title": "React: class to className"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "test": "node ./node_modules/@vscode/test-electron/out/runTest.js"
  },
  "devDependencies": {
    "@types/mocha": "^9.1.0",
    "@types/node": "^17.0.12",
    "@types/vscode": "^1.63.1",
    "@vscode/test-electron": "^2.1.1",
    "mocha": "^9.2.0",
    "typescript": "^4.5.5"
  }
}
