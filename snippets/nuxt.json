{
  "xPRO:Nuxt": {
    "prefix": "nuxt",
    "description": "nuxt tag",
    "body": ["<Nuxt />"]
  },
  "xPRO:NuxtChild": {
    "prefix": "nuxt-child",
    "description": "nuxt child",
    "body": ["<NuxtChild :key=\"123\" />"]
  },
  "xPRO:NuxtLink": {
    "prefix": "nuxt-link",
    "description": "nuxt link",
    "body": ["<NuxtLink to=\"/$0\">${1:Home page}</NuxtLink>"]
  },
  "xPRO:NuxtLink prefetch": {
    "prefix": "nuxt-prefetch",
    "description": "nuxt preFetch",
    "body": ["<NuxtLink to=\"/$0\" prefetch>${1:Home page}</NuxtLink>"]
  },
  "xPRO:NuxtLink no-prefetch": {
    "prefix": "nuxt-no-prefetch",
    "description": "nuxt no-preFetch",
    "body": ["<NuxtLink to=\"/$0\" no-prefetch>${1:Home page}</NuxtLink>"]
  },
  "xPRO:Nuxt ClintOnly": {
    "prefix": "nuxt-ClientOnly",
    "description": "nuxt ClientOnly",
    "body": ["<client-only placeholder=\"Loading...\">", "$0", "</client-only>"]
  },
  "xPRO:Nuxt head": {
    "prefix": "nuxt-head",
    "description": "nuxt head",
    "body": [
      "head: {",
      "\ttitle: 'my website title',",
      "\tmeta: [",
      "\t\t{ charset: 'utf-8' },",
      "\t\t{ name: 'viewport', content: 'width=device-width, initial-scale=1' },",
      "\t\t{",
      "\t\t\thid: 'description',",
      "\t\t\tname: 'description',",
      "\t\t\tcontent: 'my website description'",
      "\t\t}",
      "\t],",
      "\tlink: [{ rel: 'icon', type: 'image/x-icon', href: '/favicon.ico' }]",
      "}"
    ]
  }
}
